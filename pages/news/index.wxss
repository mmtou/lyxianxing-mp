.container {

}
.list {

}
.item {
  display: flex;
  align-items: center;
  padding: 24rpx;
  border-bottom: 1px solid #F0F0F0;
}
.item .left {
  flex: 1;
  height: 120rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.item .left .title {
  width: 550rpx;
  font-size: 30rpx;
  line-height: 36rpx;
  height: 72rpx;
  color: #333333;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.item .left .time-count {
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 24rpx;
  color: #999999;
  margin-top: 12rpx;
}
.item .left .time {
}

.item .right {
  width: 120rpx;
  height: 120rpx;
  margin-left: 24rpx;
  border-radius: 12rpx;
}

